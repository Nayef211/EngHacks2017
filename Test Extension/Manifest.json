{
  "name": "PaperBoy",
  "version": "0.1",
  "manifest_version":2,
  "description": "Gets information from Google.",
  "icons": 
  { 
  	"512": "NewspaperIcon.png"
  },
  "author": "Ali, Nayef, Tim, Tracy",

  "background": 
  {
    "scripts": ["background.js"]
  },


  "permissions": ["geolocation","<all_urls>", "unlimitedStorage", "tabs"],
  

  "browser_action": 
  {
    "default_title": "PaperBoy",
    "default_icon": "NewspaperIcon.png",
    "default_popup": "popup.html"
  }
}